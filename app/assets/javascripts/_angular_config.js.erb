var app = angular.module('voteYoList', ['ui.router'])

app.config(['$stateProvider', '$urlRouterProvider',function ($stateProvider, $urlRouterProvider){
  $stateProvider
    .state('home', {
      url: '/',
      templateUrl: '<%= asset_path("static_pages/index.html")%>'
  })

  $urlRouterProvider.otherwise('/');
}])
